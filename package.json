{
  "name": "isaac-cs-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.11.0",
    "@types/react": "16.8.7",
    "@types/react-dom": "16.8.2",
    "@types/react-redux": "7.0.3",
    "@types/redux-logger": "3.0.7",
    "@types/remarkable": "1.7.3",
    "axios": "0.18.0",
    "react": "16.8.4",
    "react-app-polyfill": "^0.2.2",
    "react-dom": "16.8.4",
    "react-redux": "6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "remarkable": "1.7.1",
    "typescript": "3.3.3333"
  },
  "scripts": {
    "start": "set PORT=8000 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "eslint:all",
      "plugin:react/all",
      "plugin:jsx-a11y/recommended"
